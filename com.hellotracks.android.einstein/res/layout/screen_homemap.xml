<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/frameLayout1" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/gradient">
	<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
		<FrameLayout android:id="@+id/logoFrame" android:layout_width="fill_parent" android:background="@drawable/header_bg"
			android:layout_height="50dip" android:visibility="gone">
			<TextView android:id="@+id/name" android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center_vertical|center_horizontal"
				android:text="@string/Map" android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="@dimen/title">
			</TextView>
		</FrameLayout>
		<RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
			<!--releaseKey="0TA7w9eU4emjYVcxzDR7inovmUbmhQq-OAdAh1Q" -->
			<!--debugKey = 0TA7w9eU4emg4zrvXMDhWsOljjcNse8t1l1o2gw -->
			<FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent">
				<com.google.android.maps.MapView android:id="@+id/mapview" android:layout_width="fill_parent" android:layout_height="fill_parent"
					android:clickable="true" android:apiKey="0TA7w9eU4emjYVcxzDR7inovmUbmhQq-OAdAh1Q" />
				<LinearLayout android:id="@+id/actionsContainerLeft" android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:layout_gravity="bottom|left" android:orientation="vertical">
					<ImageButton android:layout_width="60dip" android:src="@drawable/tab_jobs_white" android:layout_height="60dip" android:id="@+id/buttonMessages"
						android:background="@drawable/custom_button_trans" android:layout_gravity="top|center_horizontal" android:scaleType="centerInside"
						android:layout_margin="2dip" android:onClick="onMessages"></ImageButton>
					<ImageButton android:background="@drawable/custom_button_trans_nopadding" android:layout_width="60dip" android:src="@drawable/tab_tracks_white"
						android:layout_height="60dip" android:id="@+id/buttonTracks" android:layout_gravity="bottom|center_horizontal" android:scaleType="centerInside"
						android:layout_margin="2dip" android:onClick="onTracks"></ImageButton>
					<ImageButton android:layout_width="60dip" android:src="@drawable/btn_panic" android:layout_height="60dip" android:id="@+id/buttonMenu"
						android:background="@drawable/custom_button_trans" android:layout_gravity="top|center_horizontal" android:scaleType="centerInside"
						android:layout_margin="2dip" android:onClick="onPanic"></ImageButton>
				</LinearLayout>
				<ImageButton android:layout_width="60dip" android:src="@drawable/btn_shortmenu" android:layout_height="30dip" android:id="@+id/buttonMenu"
							android:background="@drawable/custom_button_trans_nopadding" android:scaleType="centerInside"
							android:layout_margin="2dip" android:onClick="onMenu" android:layout_gravity="bottom|center_horizontal"></ImageButton>
				<LinearLayout android:id="@+id/actionsContainerRight" android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:layout_gravity="bottom|right" android:orientation="vertical">
					<FrameLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="2dip">
						<ImageButton android:layout_width="60dip" android:src="@drawable/mode_transport" android:layout_height="60dip" android:id="@+id/buttonMode"
							android:background="@drawable/custom_button_trans_nopadding" android:layout_gravity="top|center_horizontal" android:scaleType="centerInside"
							android:layout_margin="2dip" android:onClick="onCockpit"></ImageButton>
						<TextView android:id="@+id/textMode" android:layout_width="wrap_content" android:layout_height="60dip" android:layout_gravity="bottom|center_horizontal"
							android:text="@string/Cockpit" android:textColor="#FFF" android:textSize="9sp" android:layout_marginTop="2dip"/>
					</FrameLayout>
					<FrameLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="2dip">
						<ImageButton android:layout_width="60dip" android:src="@drawable/btn_on" android:layout_height="60dip" android:id="@+id/buttonStatus"
							android:background="@drawable/custom_button_trans_green" android:layout_gravity="top|center_horizontal" android:scaleType="centerInside"
							android:layout_margin="2dip" android:onClick="onOnOff"></ImageButton>
						<TextView android:id="@+id/trackingText" android:layout_width="wrap_content" android:layout_height="60dip" android:layout_gravity="bottom|center_horizontal"
							android:text="@string/Locating" android:textColor="#FFF" android:textSize="9sp" android:layout_marginTop="2dip"/>
					</FrameLayout>
				</LinearLayout>
				<HorizontalScrollView android:orientation="horizontal" android:layout_gravity="top" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:scrollbars="none">
					<LinearLayout android:id="@+id/contactsContainer" android:layout_width="match_parent" android:layout_height="match_parent">
					</LinearLayout>
				</HorizontalScrollView>
			</FrameLayout>
			<ImageView android:id="@+id/drag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/marker"
				android:visibility="gone" />
		</RelativeLayout>
	</LinearLayout>
</FrameLayout>