<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- draw tab background -->


	<item>
		<shape android:shape="rectangle">
			<gradient android:angle="-90" android:startColor="#566D8D"
				android:endColor="#566D9D" />
			<stroke android:width="0.5dip" android:color="#66666666" />
			<corners android:bottomRightRadius="0dp"
				android:bottomLeftRadius="0dp" android:topLeftRadius="4dp"
				android:topRightRadius="4dp" />
		</shape>

	</item>

</layer-list>