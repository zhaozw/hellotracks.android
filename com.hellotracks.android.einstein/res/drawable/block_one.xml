<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" >
        <shape>
            <gradient android:angle="-90" android:startColor="#33566D8D" android:endColor="#33566D9D" />
			<stroke android:width="1dip" android:color="#66666666" />
			<corners android:radius="4dp" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
        	<gradient android:angle="-90" android:startColor="#363D4D" android:endColor="#363D5D" />
			<stroke android:width="1dip" android:color="#66666666" />
			<corners android:radius="4dp" />
		</shape>
    </item>
    <item>
        <shape>
        	<gradient android:angle="-90" android:startColor="#566D8D" android:endColor="#566D9D" />
			<stroke android:width="1dip" android:color="#66666666" />
			<corners android:radius="4dp" />
		</shape>
    </item>
</selector>

